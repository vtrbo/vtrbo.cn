{
  "prettier.enable": false,
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "svelte",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "cSpell.words": [
    "vtrbo",
    "antfu",
    "mdsvex",
    "shiki",
    "vitesse"
  ]
}
